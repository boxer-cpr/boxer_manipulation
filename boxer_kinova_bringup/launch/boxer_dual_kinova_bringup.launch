<launch>
  <arg name="left_serial_number" default="$(optenv BOXER_LEFT_KINOVA_SERIAL NOT_SET)" />
  <arg name="right_serial_number" default="$(optenv BOXER_RIGHT_KINOVA_SERIAL NOT_SET)" />
  <arg name="arm_type" default="$(optenv BOXER_KINOVA_ARM_TYPE NOT_SET)" />

  <node pkg="boxer_manipulation_description" type="launch_delay.sh"
    args="1 boxer_kinova_bringup kinova_robot.launch kinova_robotType:=$(arg arm_type) kinova_robotName:=left_kinova_arm kinova_robotSerial:=$(arg left_serial_number)"
    name="left_timed_roslaunch" output="screen"/>
  <node pkg="boxer_manipulation_description" type="launch_delay.sh"
    args="3 boxer_kinova_bringup kinova_robot.launch kinova_robotType:=$(arg arm_type) kinova_robotName:=right_kinova_arm kinova_robotSerial:=$(arg right_serial_number)"
    name="right_timed_roslaunch" output="screen"/>
</launch>
